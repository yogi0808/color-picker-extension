{
    "manifest_version": 3,
    "name": "Color Picker",
    "description": "A simple color picker extension",
    "version": "0.0.1",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    },
    "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
}
  